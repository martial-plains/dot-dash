{
    "lock": false,
    "tasks": {
        "build_css": "deno run -A npm:tailwindcss -i ./public/styles/main.css -o ./public/styles/tailwind/tailwind.css"
    },
    "imports": {
        "@tailwindcss/typography": "npm:@tailwindcss/typography@^0.5.15",
        "daisyui": "npm:daisyui@^4.12.10",
        "tailwindcss": "npm:tailwindcss@3.3.5"
    },
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "preact"
    },
    "nodeModulesDir": "auto"
}